{
  "name": "ob-aspsp-forgerock-openbanking-ui",
  "project_version": "madness",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test --passWithNoTests",
    "test.watch": "ng test --watch",
    "test.ci": "ng test --ci --bail --passWithNoTests",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix",
    "lint": "ng lint",
    "=================BANK APP=================": "",
    "bundleSize.bank": "openbanking-ui-cli appSize --theme forgerock --project bank",
    "serve.bank": "ng serve bank",
    "report.bank": "npm run build.bank.forgerock && webpack-bundle-analyzer dist/forgerock/stats.json",
    "build.bank.forgerock": "ng build --project=bank --prod --configuration=forgerock --output-path dist/forgerock --statsJson --extra-webpack-config webpack.extra.js",
    "build.bank.themes": "openbanking-ui-cli buildThemes --project bank",
    "=================REGISTER APP=================": "",
    "bundleSize.manual-onboarding": "openbanking-ui-cli appSize --theme forgerock --project manual-onboarding",
    "serve.manual-onboarding": "ng serve manual-onboarding",
    "report.manual-onboarding": "npm run build.manual-onboarding.forgerock && webpack-bundle-analyzer dist/forgerock/stats.json",
    "build.manual-onboarding.forgerock": "ng build --project=manual-onboarding --prod --configuration=forgerock --output-path dist/forgerock --statsJson --extra-webpack-config webpack.extra.js",
    "build.manual-onboarding.themes": "openbanking-ui-cli buildThemes --project manual-onboarding",
    "=================CLUSTER UI-INTEG=================": "",
    "serve.ui-integ.bank": "ng serve bank --host dev.bank.ui-integ.forgerock.financial --browserTarget bank:build:ui-integ --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key",
    "serve.ui-integ.manual-onboarding": "ng serve manual-onboarding --host dev.register.ui-integ.forgerock.financial --browserTarget manual-onboarding:build:ui-integ --sslCert ssl/all.ui-integ.forgerock.financial.crt --sslKey ssl/k-all.ui-integ.forgerock.financial.key"
  },
  "private": true,
